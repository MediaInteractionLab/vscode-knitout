{
    "name": "vscode-knitout",
    "displayName": "knitout",
    "description": "knitout language extension for VS Code",
    "version": "0.0.1",
    "publisher": "ithrak",
    "author": {
        "name": "iko79"
    },
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#C80000",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.53.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "repository": {
        "type": "git",
        "url": "https://gitlab.fh-ooe.at/fe/milab/vscode-knitout.git"
    },
    "homepage": "https://www.mi-lab.org",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "knitout",
            "aliases": ["knitout", "knitout"],
            "extensions": [".k",".knitout"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "knitout",
            "scopeName": "source.knitout",
            "path": "./syntaxes/knitout.tmLanguage.json"
        }],
        "themes": [
            {
                "label": "knitout-dark",
                "uiTheme": "vs-dark",
                "path": "./themes/knitout-vs-dark-color-theme.json"
            },
            {
                "label": "knitout",
                "uiTheme": "vs",
                "path": "./themes/knitout-vs-color-theme.json"
            }
        ]
    }
}